<% layout('layouts/boilerplate') %>
    <div class="alert alert-danger" role="alert">
        <small>Error Code: <%= error.statusCode %></small>
        <h4 class="alert-heading">
            <%= error.message %>
        </h4>
        <p>
            <%= error.detail %>
        </p>
        <hr>
        <p class="mb-0">
            <%= error.stack %>
        </p>
    </div>